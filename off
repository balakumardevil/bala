#include <stdio.h>
int main()
{
    int y;

    printf("Enter year: ");
    scanf("%d",&y);

    if(y % 4 == 0)
    {
        if( y % 100 == 0)
        {
            if ( y % 400 == 0)
                printf("%d YES ", y);
            else
                printf("%d NO", y);
        }
        else
            printf("%d YES", y );
    }
    else
        printf("%d NO ", y);

    return 0;
}
