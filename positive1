import java.util.*;
class add 
{
public static void main(String[] arg)
{
int a;
Scanner s=new Scanner(System.in);
System.out.println("enter the element");
a=s.nextInt();
if(a>0)
{
System.out.println("positive");
}
else if(a<0)
{
System.out.println("negative");
}
else
{
System.out.println("zero");
}
}
}
